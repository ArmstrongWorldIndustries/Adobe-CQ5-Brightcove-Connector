<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    title="Dialog"
    xtype="dialog">
    <items jcr:primaryType="cq:TabPanel">
        <items jcr:primaryType="cq:WidgetCollection">
            <tab1
                jcr:primaryType="cq:Panel"
                title="General">
                <items jcr:primaryType="cq:WidgetCollection">
                    <account
                        jcr:primaryType="cq:Widget"
                        path="/apps/brightcove/components/shared/dialog/accountSelection.infinity.json"
                        xtype="cqinclude"/>
                    <videoPlayerPL
                        jcr:primaryType="cq:Widget"
                        path="/apps/brightcove/components/shared/dialog/playlistCombobox.infinity.json"
                        xtype="cqinclude"/>
                </items>
            </tab1>
            <tab2
                jcr:primaryType="cq:Panel"
                title="Account Settings">
                <items jcr:primaryType="cq:WidgetCollection">
                    <playerPath
                        jcr:primaryType="cq:Widget"
                        path="/apps/brightcove/components/shared/dialog/playerCombobox.infinity.json"
                        xtype="cqinclude"/>
                </items>
            </tab2>
        </items>
    </items>
</jcr:root>
